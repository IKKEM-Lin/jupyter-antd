{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build -c vite-prod.config.js --mode=Button && vite build -c vite-prod.config.js --mode=Input && vite build -c vite-prod.config.js --mode=Radio && vite build -c vite-prod.config.js --mode=Select && vite build -c vite-prod.config.js --mode=FileSelector && vite build -c vite-prod.config.js --mode=Formily",
    "build:button": "tsc && vite build -c vite-prod.config.js -w --mode=Button",
    "build:input": "tsc && vite build -c vite-prod.config.js -w --mode=Input",
    "build:radio": "tsc && vite build -c vite-prod.config.js -w --mode=Radio",
    "build:select": "tsc && vite build -c vite-prod.config.js -w --mode=Select",
    "build:fileSelector": "tsc && vite build -c vite-prod.config.js -w --mode=FileSelector",
    "build:formily": "tsc && vite build -c vite-prod.config.js -w --mode=Formily",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@anywidget/react": "^0.0.2",
    "@formily/antd-v5": "^1.1.7",
    "@formily/core": "^2.2.29",
    "@formily/react": "^2.2.29",
    "@types/node": "^20.8.4",
    "antd": "^5.10.0",
    "less": "^4.2.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.51.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prop-types": "^15.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0-beta.4"
  }
}
